    "dependencies": {
        "@sap/cds": "^3.17.4",
        "@sap/cds-odata-v2-adapter-proxy": "^1.4.1",
        "express": "^4.17.1"
    },
    "engines": {
        "node": "^8.9"
    },
    "scripts": {
        "build": "cds build/all --clean",
        "deploy": "cds deploy",
        "start": "cds run",
        "startv2": "node v2server",
        "debug": "node --inspect node_modules/@sap/cds/bin/cds.js run"
    },
    "files": [
        "srv",
        "db"
    ],
    "cds": {
        "odata": {
            "version": "v4"
