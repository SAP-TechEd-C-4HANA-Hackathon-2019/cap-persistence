        "requires": {
            "db": {
                "kind": "sqlite",
                "pool": { "max": 1 },
                "model": [
                    "srv",
                    "db"
                ],
                "credentials": {
                    "database": "sqlite.db"
                }
            }
        }
    },
    "devDependencies": {
        "sqlite3": "^4.1.0"
    }
